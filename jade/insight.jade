mixin insight(place)
    include header
    +header('../')

    section.insight
        h1 #{place.name} 
        h2 #{place.date}
        
        hr
        
        .places.js-places
            - for (var i = 1; i <= place.size; i++)
                //- .img.js-img(data-src="#{i}.jpg")
                img.img.js-img(src="https://images.unsplash.com/photo-1437652633673-cc02b9c67a1b?dpr=2&auto=format&fit=crop&w=1500&h=1001&q=80&cs=tinysrgb&crop=&bg=")
        
