mixin insight(place)
    +header('../', 'Insight')

    section.insight
        h1 #{place.name} 
        h2 #{place.date}
        
        hr
        
        .places.js-places
            - for (var i = 1; i <= place.size; i++)
                a(href='../imgs/#{place.folder}/#{i}.jpg')
                  img(src="../imgs/#{place.folder}/#{i}.jpg")
        
